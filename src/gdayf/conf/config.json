{ "frameworks":
  {"h2o":
    {
      "path_localfs" : "D:/Data/models",
      "path_hdfs" : null,
      "path_mongoDB" : null,
      "primary_path" : "path_localfs",
      "url" : "http://127.0.0.1:54321",
      "nthreads" : 4,
      "ice_root" : "D:/Data/logs",
      "max_mem_size" : "8G",
      "start_h2o" : true,
      "debug" : true,
      "save_model" : "NATIVE"
    }
  },
  "logging":
  {
    "version": 1,
    "disable_existing_loggers": true,
    "level": "DEBUG",
    "formatters": {
        "standard": {
            "format": "%(asctime)s\t%(levelname)s\t%(module)s\t%(message)s",
            "datefmt": "%Y-%m-%d %H:%M:%S %z"
        }
    },
    "handlers": {
        "info": {
            "class": "logging.handlers.RotatingFileHandler",
            "formatter": "standard",
            "filename": "D:\\Data\\logs\\1-DayF-info.log",
            "level": "INFO",
            "maxBytes": 10485760,
            "backupCount": 5,
            "encoding": "utf8"
          },
          "debug": {
              "class": "logging.FileHandler",
              "formatter": "standard",
              "filename": "D:\\Data\\logs\\2-DayF-debug.log",
              "level": "DEBUG",
              "encoding": "utf8"
            },
            "error": {
              "class": "logging.handlers.RotatingFileHandler",
              "formatter": "standard",
              "filename": "D:\\Data\\logs\\3-DayF-error.log",
              "level": "ERROR",
              "maxBytes": 10485760,
              "backupCount": 5,
              "encoding": "utf8"
            }
    },
    "loggers": {
        "logs.logHandler": {
            "handlers": ["error"],
            "propagate": true
        },
        "logs": {
            "handlers": ["info"],
            "propagate": false
        },
        "engines.H2OHandler": {
            "handlers": ["error"],
            "propagate": true
        },
        "engines.h2o": {
            "handlers": ["info"],
            "propagate": false
        }
    }
    }
}
